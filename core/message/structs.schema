struct Message {
	Time    int64
	From    string
	To      string
	ReqType int32
	TTL     int8
	Body    []byte
}

struct RequestHeight  {
	LocalBlockHeight uint64
	NeedBlockHeight  uint64
}

struct ResponseHeight  {
	BlockHeight  uint64
}

struct RequestBlock {
	BlockNumber  uint64
}

struct BlockHashQuery {
    Start   uint64
    End     uint64
}

struct BlockHash {
    Height  uint64
    Hash    []byte
}

struct BlockHashResponse {
    BlockHashes []BlockHash
}
